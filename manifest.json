    "manifest_version": 4,
    "type": "extension",
    "name": "MoreMemes",
    "slug": "MoreMemes",
    "description": "更大的表情包容量！",
    "version": "0.1.0",
    "icon": "./icon.png",
    "thumb": "./icon.svg",
    "authors": [
        {
            "name": "香鱼Ayubass",
            "link": "https://github.com/ImXiangYu"
        }
    ],
    "platform": [
